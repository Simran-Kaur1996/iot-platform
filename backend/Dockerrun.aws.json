{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "iot-backend",
      "image": "121955270316.dkr.ecr.us-east-1.amazonaws.com/iot-backend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 5000
        }
      ]
    },
    {
      "name": "iot-frontend",
      "image": "121955270316.dkr.ecr.us-east-1.amazonaws.com/iot-frontend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3000
        }
      ]
    }
  ]
}
