{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "iot-backend",
      "image": "simran31/iot-backend:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ]
    },
    {
      "name": "iot-frontend",
      "image": "simran31/iot-frontend:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ]
}
